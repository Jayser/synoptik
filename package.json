{
  "name": "synoptik",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "watch": "webpack --colors --watch",
    "server": "better-npm-run server",
    "build:dist": "better-npm-run build:dist",
    "build:prod": "better-npm-run build:prod",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test"
  },
  "betterScripts": {
    "server": {
      "command": "node server.js"
    },
    "build:dist": {
      "command": "webpack --display-modules"
    },
    "build:prod": {
      "command": "webpack --config $npm_package_webpack --progress --colors",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "test": {
      "command": "karma start --single-run --no-auto-watch karma.conf.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "karma start --auto-watch --no-single-run karma.conf.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jayser/synoptik.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/Jayser/synoptik/issues"
  },
  "homepage": "https://github.com/Jayser/synoptik#readme",
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone.localstorage": "^1.1.16",
    "font-awesome": "^4.6.3",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.3.0",
    "jquery": "<2",
    "lodash": "^4.15.0",
    "skycons": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.11",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.5.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.8.0",
    "open": "0.0.5",
    "phantomjs-polyfill": "0.0.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.15.0"
  }
}
